{
    "editor.fontFamily": "'Fira Code', 'Inconsolata', 'Source Code Pro', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "terminal.integrated.rendererType": "dom",
    "editor.tabSize": 4,
    "files.insertFinalNewline": true,
    "doxdocgen.file.versionTag": "",
    "doxdocgen.generic.authorEmail": "",
    "doxdocgen.generic.authorTag": "",
    "doxdocgen.generic.dateFormat": "",
    "doxdocgen.file.copyrightTag": [],
    "files.watcherExclude": {
        "**/target/**": true
    },
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "workbench.tree.renderIndentGuides": "always",
    "editor.rulers": [
        80
    ],
    "latex-workshop.intellisense.package.enabled": true,
    "latex-workshop.latex.external.build.command": "make",
    "latex-workshop.view.pdf.viewer": "tab",
    "code-eol.crlfCharacter": "↵↓",
    "code-eol.newlineCharacter": "↵",
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "workbench.colorTheme": "Visual Studio Light",
    "editor.fontSize": 15,
    "terminal.integrated.allowChords": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.insertSpaces": false,
}

{
    "editor.fontFamily": "'badfont', 'Victor Mono Medium', 'Fira Code', 'Inconsolata', 'Source Code Pro', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": false,
    "terminal.integrated.rendererType": "dom",
    "doxdocgen.file.versionTag": "",
    "doxdocgen.generic.authorEmail": "",
    "doxdocgen.generic.authorTag": "",
    "doxdocgen.generic.dateFormat": "",
    "doxdocgen.file.copyrightTag": [],
    "files.watcherExclude": {
        "**/target/**": true
    },
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "workbench.tree.renderIndentGuides": "always",
    "editor.rulers": [
        80
    ],
    "latex-workshop.intellisense.package.enabled": true,
    "latex-workshop.latex.external.build.command": "make",
    "latex-workshop.view.pdf.viewer": "tab",
    "code-eol.crlfCharacter": "↵↓",
    "code-eol.newlineCharacter": "↵",
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "editor.fontSize": 23,
    "terminal.integrated.allowChords": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.insertSpaces": false,
    "print.printAndClose": false,
    "print.colourScheme": "vs",
    "print.fontSize": "9pt",
    "diffEditor.renderSideBySide": false,
    "workbench.iconTheme": "Monokai Pro Icons",
    "code-eol.highlightExtraWhitespace": true,
    "code-eol.highlightNonDefault": true,
    "editor.autoClosingBrackets": "never",
    "C_Cpp.suggestSnippets": false,
    "editor.wordBasedSuggestions": false,
    "window.menuBarVisibility": "toggle",
    "workbench.colorTheme": "Visual Studio Dark Theme",
    "files.insertFinalNewline": true,
    "terminal.integrated.fontSize": 28,
	"terminal.integrated.env.linux": {
		"visual_studio": "true"
	}
}

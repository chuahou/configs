{
    "editor.fontFamily": "'Victor Mono Medium', 'Fira Code', 'Inconsolata', 'Source Code Pro', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "terminal.integrated.rendererType": "dom",
    "files.insertFinalNewline": true,
    "doxdocgen.file.versionTag": "",
    "doxdocgen.generic.authorEmail": "",
    "doxdocgen.generic.authorTag": "",
    "doxdocgen.generic.dateFormat": "",
    "doxdocgen.file.copyrightTag": [],
    "files.watcherExclude": {
        "**/target/**": true
    },
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "workbench.tree.renderIndentGuides": "always",
    "editor.rulers": [
        80
    ],
    "latex-workshop.intellisense.package.enabled": true,
    "latex-workshop.latex.external.build.command": "make",
    "latex-workshop.view.pdf.viewer": "tab",
    "code-eol.crlfCharacter": "↵↓",
    "code-eol.newlineCharacter": "↵",
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "workbench.colorTheme": "Monokai Pro (Filter Machine)",
    "editor.fontSize": 16,
    "terminal.integrated.allowChords": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.insertSpaces": false,
    "printcode.disableTelemetry": true,
    "printcode.fontSize": 10,
    "printcode.printFilePath": "none",
    "printcode.tabSize": 4,
    "print.printAndClose": false,
    "print.colourScheme": "vs",
    "print.fontSize": "9pt",
    "diffEditor.renderSideBySide": false,
    "workbench.iconTheme": "Monokai Pro (Filter Machine) Icons",
    "code-eol.highlightExtraWhitespace": true,
    "code-eol.highlightNonDefault": true,
}

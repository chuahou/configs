{
    "workbench.colorTheme": "Visual Studio Light",
    "editor.fontFamily": "'Inconsolata', 'Source Code Pro', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "terminal.integrated.rendererType": "dom",
    "editor.tabSize": 4,
    "terminal.integrated.fontSize": 18,
    "files.insertFinalNewline": true,
    "doxdocgen.file.versionTag": "",
    "doxdocgen.generic.authorEmail": "",
    "doxdocgen.generic.authorTag": "",
    "doxdocgen.generic.dateFormat": "",
    "doxdocgen.file.copyrightTag": [],
    "files.watcherExclude": {
        "**/target/**": true
    },
    "editor.fontSize": 18,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": true,
    "workbench.tree.renderIndentGuides": "always",
}
